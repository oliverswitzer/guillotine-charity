{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier", "jest"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-props-no-spreading": 0,
        "no-use-before-define": "off"
    },
    "env": {
        "jest/globals": true,
        "node": true,
        "browser": true,
    },
    "globals": {
        "document": false
    },
    "parser": "babel-eslint",
}
